<h2>Show project</h2>

<ul>

  <li>
    <strong>Slug:</strong>
    <%= @project.slug %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @project.name %>
  </li>

  <li>
    <strong>Image:</strong>
    <%= @project.image %>
  </li>

  <li>
    <strong>Content:</strong>
    <%= @project.content %>
  </li>

</ul>

<%= link "Edit", to: project_path(@conn, :edit, @project) %>
<%= link "Back", to: project_path(@conn, :index) %>
